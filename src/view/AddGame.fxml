<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onKeyPressed="#exit" prefHeight="397.0" prefWidth="277.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddGameController">
   <children>
      <Label layoutX="21.0" layoutY="67.0" text="Name" textFill="WHITE" />
      <TextField fx:id="name" layoutX="65.0" layoutY="62.0" onKeyPressed="#enterFromName" />
      <TextField fx:id="path" editable="false" layoutX="14.0" layoutY="122.0" prefHeight="26.0" prefWidth="171.0" />
      <Label layoutX="37.0" layoutY="104.0" text="Path to the game (only .jar)" textFill="WHITE" />
      <Button layoutX="185.0" layoutY="123.0" mnemonicParsing="false" onAction="#browse" onKeyPressed="#enterBrowse" text="Browse.." />
      <Label layoutX="14.0" layoutY="199.0" text="Remember that if you want an image" textFill="WHITE" wrapText="true" />
      <Label layoutX="14.0" layoutY="222.0" text="as preview the image MUST " textFill="WHITE" />
      <Label layoutX="14.0" layoutY="246.0" text="be in the same directory of the game" textFill="WHITE" />
      <Button layoutX="21.0" layoutY="323.0" mnemonicParsing="false" onAction="#cancel" onKeyPressed="#enterCancel" text="Cancel" />
      <Button layoutX="188.0" layoutY="323.0" mnemonicParsing="false" onAction="#submit" onKeyPressed="#enterSubmit" text="Submit" />
      <Label layoutX="14.0" layoutY="273.0" text="and MUST be called &quot;preview.png&quot;" textFill="WHITE" />
   </children>
</AnchorPane>
