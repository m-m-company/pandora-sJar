<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="397.0" prefWidth="277.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddGameController">
   <children>
      <Label layoutX="21.0" layoutY="67.0" text="Name" />
      <TextField fx:id="name" layoutX="65.0" layoutY="62.0" />
      <TextField fx:id="path" editable="false" layoutX="14.0" layoutY="122.0" prefHeight="26.0" prefWidth="171.0" />
      <Label layoutX="14.0" layoutY="106.0" text="Path to the game (*.jar, *.exe ect)" />
      <Button layoutX="185.0" layoutY="122.0" mnemonicParsing="false" onAction="#browse" text="Browse.." />
      <Label layoutX="14.0" layoutY="176.0" text="Remember that if you want an image" wrapText="true" />
      <Label layoutX="14.0" layoutY="199.0" text="as preview the image MUST " />
      <Label layoutX="14.0" layoutY="223.0" text="be in the same directory of the game" />
      <Button layoutX="190.0" layoutY="293.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Label layoutX="14.0" layoutY="250.0" text="and MUST be called &quot;preview.png&quot;" />
      <Button layoutX="21.0" layoutY="293.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
